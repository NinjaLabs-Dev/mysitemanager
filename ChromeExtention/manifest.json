{

    "manifest_version": 2,
    "name": "MySiteManager",
    "description": "quick add",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens popup.html"
        }
    }


}